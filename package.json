    "@monaco-editor/react": "^4.3.1",
    "@nestjs/cli": "^8.1.6",
    "@nestjs/schematics": "^8.0.5",
    "@nestjs/testing": "^8.2.4",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.0",
